#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/cli/bin/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/cli/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/cli/bin/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/cli/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/cli/bin/nest.js" "$@"
else
  exec node  "$basedir/../.pnpm/@nestjs+cli@11.0.10_@types+node@22.19.0/node_modules/@nestjs/cli/bin/nest.js" "$@"
fi
