#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Sistemas/chGit/node_modules/.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules/jest/bin/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules/jest/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Sistemas/chGit/node_modules/.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules/jest/bin/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules/jest/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../.pnpm/jest@30.2.0_@types+node@22._c05824d95926df1d5cb28ba64cfa8161/node_modules/jest/bin/jest.js" "$@"
fi
