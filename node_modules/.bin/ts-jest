#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Sistemas/chGit/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_be4b9e8b380440fca23b8892473f3d8c/node_modules/ts-jest/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_be4b9e8b380440fca23b8892473f3d8c/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Sistemas/chGit/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_be4b9e8b380440fca23b8892473f3d8c/node_modules/ts-jest/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@_be4b9e8b380440fca23b8892473f3d8c/node_modules:/mnt/c/Sistemas/chGit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.5_@babel+core@_be4b9e8b380440fca23b8892473f3d8c/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.5_@babel+core@_be4b9e8b380440fca23b8892473f3d8c/node_modules/ts-jest/cli.js" "$@"
fi
